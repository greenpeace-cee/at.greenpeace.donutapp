<?xml version="1.0"?>
<extension key="at.greenpeace.donutapp" type="module">
  <file>donutapp</file>
  <name>Donutapp</name>
  <description>API Integration with Donutapp by Formunauts</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Patrick Figel</author>
    <email>pfigel@greenpeace.org</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/greenpeace-cee/at.greenpeace.donutapp</url>
    <url desc="Documentation">https://github.com/greenpeace-cee/at.greenpeace.donutapp</url>
    <url desc="Support">https://github.com/greenpeace-cee/at.greenpeace.donutapp/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2026-02-05</releaseDate>
  <version>2.7.0-alpha.1</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.69</ver>
  </compatibility>
  <comments/>
  <civix>
    <namespace>CRM/Donutapp</namespace>
    <format>23.02.1</format>
